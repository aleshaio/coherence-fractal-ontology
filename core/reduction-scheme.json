{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Coordinate Grid Reduction Schema",
  "type": "object",
  "properties": {
    "dimension": {
      "type": "string",
      "enum": ["0D","1D","2D","3D","4D","5D"]
    },
    "primitive": {
      "type": "string"
    },
    "planes": {
      "type": "array",
      "items": { "type": "string" }
    },
    "ops": {
      "type": "array",
      "items": { "type": "string" }
    },
    "evidence": { "type": "string" },
    "confidence": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    }
  },
  "required": ["dimension","primitive","planes","ops"]
}
