  "funding": {
    "sources": ["Funding sources if applicable"],
    "amount": "Amount if relevant and disclosable",
    "constraints": ["Any constraints funding imposes"],
    "acknowledgments": ["People or institutions to acknowledge"]
  },

  "license": {
    "data": "License for data (CC0, CC-BY, etc.)",
    "code": "License for code (MIT, GPL, etc.)",
    "content": "License for research content (CC-BY, etc.)",
    "note": "Encourage open licensing aligned with CFO civic values"
  }
}