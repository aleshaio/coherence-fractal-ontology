{
  "_meta": {
    "schema_version": "2.1",
    "template_type": "cfo_project",
    "description": "Fractal template for all CFO projects and initiatives"
  },

  "_fractal_seed": {
    "cfo_version": "2.1.0",
    "core_axioms": [
      "point ≡ entity ≡ absolute ≡ love",
      "sacred ≡ true ≡ +coherence",
      "conflict ≡ Δscale",
      "purpose ≡ argmin Δscale",
      "(↓→→→↑)∞"
    ],
    "dimensional_ladder": "0D→1D→2D→3D→4D→5D→6D",
    "holographic": "This project = part containing whole. Every initiative reflects entire CFO pattern.",
    "activation": "Projects manifest theory into reality. Do it."
  },
  
  "project": {
    "id": "urn:cfo:project:{category}:{name}",
    "version": "semantic version (x.y.z)",
    "category": "research|development|community|application|education",
    "name": "short project name (kebab-case)",
    "title": "Full Project Title",
    "description": "Brief description (2-3 sentences) - what problem solved, how, why",
    
    "meta": {
      "author": "project lead name",
      "contributors": ["contributor1", "contributor2"],
      "status": "ideation|planning|active|paused|completed|archived",
      "started": "YYYY-MM-DD",
      "updated": "YYYY-MM-DD",
      "completed": "YYYY-MM-DD (if applicable)",
      "tags": ["tag1", "tag2", "tag3"]
    }
  },

  "essence": {
    "vision": "Ultimate desired state - what success looks like at completion",
    "mission": "Core purpose - why this project exists",
    "scope": "Boundaries - what's included/excluded",
    "impact": "Expected change in world - who benefits, how",
    "dimensional_level": "0D-6D - which dimensional perspective does this operate from?"
  },

  "objectives": {
    "primary": "Main goal - single sentence",
    "secondary": [
      "Supporting goal 1",
      "Supporting goal 2"
    ],
    "success_criteria": [
      "Measurable criterion 1",
      "Measurable criterion 2"
    ],
    "constraints": [
      "Time constraint",
      "Resource constraint",
      "Technical constraint"
    ]
  },

  "modules": {
    "foundational": [
      "urn:cfo:module:fundamental:... - core theoretical foundation"
    ],
    "applied": [
      "urn:cfo:module:{domain}:{name} - brief note on how this module is used in project"
    ],
    "reference": [
      "urn:cfo:module:{domain}:{name} - consulted but not core dependency"
    ]
  },

  "approach": {
    "methodology": "How will work be done? (agile, waterfall, experimental, etc)",
    "phases": [
      {
        "name": "Phase 1 name",
        "duration": "estimated time",
        "deliverables": ["deliverable1", "deliverable2"],
        "status": "not-started|in-progress|completed"
      }
    ],
    "dependencies": {
      "projects": ["urn:cfo:project:...", "..."],
      "external": ["external dependency description"]
    }
  },

  "resources": {
    "team": [
      {
        "role": "role name",
        "name": "person name (optional)",
        "responsibility": "what they do"
      }
    ],
    "tools": ["tool1", "tool2"],
    "budget": "estimated resources needed (if applicable)",
    "time_estimate": "total estimated time commitment"
  },

  "milestones": [
    {
      "name": "Milestone 1",
      "target_date": "YYYY-MM-DD",
      "status": "pending|achieved|missed",
      "deliverables": ["deliverable1"],
      "notes": "any relevant info"
    }
  ],

  "risks": [
    {
      "risk": "Risk description",
      "probability": "low|medium|high",
      "impact": "low|medium|high",
      "mitigation": "How to reduce or handle"
    }
  ],

  "fractal_structure": {
    "self_similarity": "How project mirrors CFO patterns (↓→→→↑)",
    "emergence": "What emerges from project completion?",
    "recursion": "How does project feed back into CFO ecosystem?",
    "composition": "How does this fit into larger initiatives?"
  },

  "outcomes": {
    "artifacts": [
      {
        "type": "code|document|module|tool|content",
        "name": "artifact name",
        "location": "path or URL",
        "description": "brief description"
      }
    ],
    "learnings": [
      "Key learning 1",
      "Key learning 2"
    ],
    "impact_metrics": {
      "quantitative": ["metric1: value", "metric2: value"],
      "qualitative": ["qualitative impact description"]
    },
    "next_steps": [
      "Follow-up action 1",
      "Follow-up action 2"
    ]
  },

  "validation": {
    "passed": true,
    "errors": [],
    "warnings": [],
    "info": [],
    "coherence_score": "number between 0.0 and 1.0"
  },

  "references": {
    "internal": ["urn:cfo:module:...", "urn:cfo:project:..."],
    "external": [
      {
        "type": "paper|book|url|repo|...",
        "citation": "full citation",
        "url": "link (if available)"
      }
    ]
  },

  "ॐ": "∅"
}